{
  "name": "@itwin/select-tool-extension-sample",
  "description": "Sample select tool extension",
  "main": "./dist/index.js",
  "module": "./lib/loader.js",
  "typings": "./lib/loader",
  "private": true,
  "license": "MIT",
  "version": "0.0.1",
  "scripts": {
    "build": "run-p build:**",
    "build:tsc": "tsc --project ./src 1>&2",
    "build:esbuild": "esbuild src/index.ts --sourcemap --bundle --outfile=dist/index.js --format=esm --target=es2020 --external:os --minify",
    "watch": "run-p \"build:** -- --watch\"",
    "debug": "serve .",
    "clean": "rimraf lib dist"
  },
  "author": {
    "name": "Bentley Systems, Inc.",
    "url": "http://www.bentley.com"
  },
  "devDependencies": {
    "@itwin/build-tools": "^3.0.0-extension.1",
    "esbuild": "^0.13.13",
    "rimraf": "^3.0.2",
    "serve": "^13.0.2",
    "typescript": "~4.4.0",
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "@bentley/itwin-client": "^3.0.0-extension.1",
    "@itwin/appui-abstract": "^3.0.0-extension.1",
    "@itwin/core-bentley": "^3.0.0-extension.1",
    "@itwin/core-common": "^3.0.0-extension.1",
    "@itwin/core-extension": "^3.0.0-extension.1",
    "@itwin/core-geometry": "^3.0.0-extension.1",
    "@itwin/core-i18n": "^3.0.0-extension.1",
    "buffer": "^6.0.3",
    "url": "^0.11.0"
  },
  "activationEvents": [
    "onStartupApp"
  ]
}